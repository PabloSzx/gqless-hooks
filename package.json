{
  "name": "gqless-hooks",
  "version": "0.1.8",
  "keywords": [
    "gqless",
    "graphql",
    "react",
    "react-hooks",
    "hooks",
    "graphql-client",
    "gql"
  ],
  "license": "MIT",
  "author": "PabloSzx",
  "main": "dist/index.js",
  "module": "dist/gqless-hooks.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdx build",
    "generate": "concurrently -s first -k \"ts-node -P ./tsconfig.node.json ./test/server\" \"wait-on tcp:9999 && gqless generate\"",
    "lint": "tsdx lint src --fix",
    "prepare": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src --fix"
    }
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0"
  },
  "devDependencies": {
    "@gqless/cli": "^0.0.1-alpha.31",
    "@nexus/schema": "^0.13.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/body-parser": "^1.19.0",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/shelljs": "^0.8.7",
    "@types/testing-library__jest-dom": "^5.0.3",
    "@types/testing-library__react": "^10.0.1",
    "@types/testing-library__react-hooks": "^3.2.0",
    "@types/wait-on": "^4.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "express-gql": "^0.0.4",
    "gqless": "^0.0.1-alpha.28",
    "graphql": "^14.6.0",
    "husky": "^4.2.3",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shelljs": "^0.8.3",
    "ts-node": "^8.8.1",
    "tsdx": "^0.13.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.1"
  },
  "peerDependencies": {
    "gqless": "^0.0.1-alpha.28",
    "graphql": "^15.0.0",
    "react": ">=16.8"
  },
  "engines": {
    "node": ">=10"
  },
  "gqless": {
    "url": "http://localhost:9999/graphql",
    "outputDir": "test/generated/graphql"
  }
}
