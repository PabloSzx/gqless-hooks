{
  "name": "gqless-hooks",
  "version": "2.0.0",
  "keywords": [
    "gqless",
    "graphql",
    "react",
    "react-hooks",
    "hooks",
    "graphql-client",
    "gql"
  ],
  "license": "MIT",
  "author": "PabloSzx",
  "main": "dist/index.js",
  "module": "dist/gqless-hooks.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdx build",
    "generate": "concurrently -s first -k \"ts-node -P ./test/tsconfig.json ./test/server\" \"wait-on tcp:9999 && gqless generate\"",
    "lint": "tsdx lint src --fix",
    "prepublishOnly": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test",
    "typedoc": "typedoc && touch ./docs/.nojekyll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src --fix"
    }
  },
  "resolutions": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0"
  },
  "devDependencies": {
    "@gqless/cli": "^0.0.1-alpha.31",
    "@nexus/schema": "^0.13.1",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/body-parser": "^1.19.0",
    "@types/concurrently": "^5.2.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/testing-library__jest-dom": "^5.0.4",
    "@types/testing-library__react": "^10.0.1",
    "@types/testing-library__react-hooks": "^3.2.0",
    "@types/wait-on": "^4.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "express-gql": "^0.0.4",
    "fetch-mock": "^9.5.1",
    "gqless": "^0.0.1-alpha.29",
    "graphql": "^14.6.0",
    "husky": "^4.2.5",
    "lorem-ipsum": "^2.0.3",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "react-use": "^14.2.0",
    "ts-node": "^8.10.1",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.2",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3",
    "waait": "^1.0.5",
    "wait-for-expect": "^3.0.2",
    "wait-on": "^5.0.0"
  },
  "peerDependencies": {
    "gqless": "^0.0.1-alpha.29",
    "graphql": "^14.6.0",
    "react": ">=16.8"
  },
  "engines": {
    "node": ">=10"
  },
  "gqless": {
    "url": "http://localhost:9999/graphql",
    "outputDir": "./test/generated/graphql"
  }
}
